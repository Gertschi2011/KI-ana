{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "KI_ana API (Uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "netapi.app:app",
        "--host", "0.0.0.0",
        "--port", "8000",
        "--reload"
      ],
      "jinja": true,
      "cwd": "${workspaceFolder}/ki_ana",
      "envFile": "${workspaceFolder}/ki_ana/.env",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
