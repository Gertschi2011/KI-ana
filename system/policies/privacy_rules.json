      {"name":"phone","regex":"\\b\\+?\\d[\\d\\s\\-()]{6,}\\b","flags":""},
      {"name":"iban","regex":"\\b[A-Z]{2}\\d{2}[A-Z0-9]{1,30}\\b","flags":""}
    ],
    "replacement": "[REDACTED]"
  },
  "exempt_domains": ["wikipedia.org","de.wikipedia.org","britannica.com"]
}
